{
  "manifest_version": 3,
  "name": "Facebook ad replacer",
  "description": "Replace facebook ads with other content",
  "version": "1.0",
  "icons": {
    "128": "images/icon-128.png",
    "512": "images/icon-512.png"
  },
  "content_scripts": [
    {
        "js": ["scripts/content.js"],
        "matches": [
            "https://www.facebook.com/*",
            "https://facebook.com/*",
            "https://m.facebook.com/*"
        ]
    }
  ]
}